@model SearchMvc.Models.SearchRequest

@{
    ViewBag.Title = "Search Page";
}

<h1>Search MVC</h1>
<form action="/" method="post">
    <div class="form-group">
        <label asp-for="Keywords"></label>
        <input class="form-control" asp-for="Keywords" placeholder="Enter keywords" />
        <span asp-validation-for="Keywords"></span>
    </div>
    <div class="form-group">
        <label asp-for="Url"></label>
        <input class="form-control" asp-for="Url" placeholder="Url" />
        <span asp-validation-for="Url"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
